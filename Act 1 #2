//Review Act 1. T55 #2
#include <stdio.h>
int main()
{
    char name[25];
    int age;
    printf("Name: ");
    fgets(name, sizeof(name), stdin);
    printf("Age: ");
    scanf("%d", &age);

    if(age<=17)
        printf("You are not eligible to vote.");
    else if(age>=18)
        printf("You are eligible to vote.");
    else
        printf("invalid age, please try again.");
    return 0;
}
