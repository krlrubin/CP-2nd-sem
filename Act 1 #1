//Review Act 1. T55 #1
#include <stdio.h>
int main() 
{
    float num1,num2;
    printf("Please enter any 2 integer:");
    scanf("%f%f", &num1,&num2);
    
    printf("ADD = %.0f\n", num1+num2);
    
    printf("MUL = %.0f\n", num1*num2);
    
    printf("DIV = %.1f\n", num1/num2);
    
    printf("SUB = %.0f\n", num1-num2);
    return 0;
}
